<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>ClienteServlet</servlet-name>
    <servlet-class>com.cibertec.servlet.ClienteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ClienteServlet</servlet-name>
    <url-pattern>/cliente</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>PeliculaServlet</servlet-name>
    <servlet-class>com.cibertec.servlet.PeliculaServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PeliculaServlet</servlet-name>
    <url-pattern>/pelicula</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AlquilerServlet</servlet-name>
    <servlet-class>com.cibertec.servlet.AlquilerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AlquilerServlet</servlet-name>
    <url-pattern>/alquiler</url-pattern>
  </servlet-mapping>
</web-app>
